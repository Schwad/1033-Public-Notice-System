<div class="container">
  <div class="row">
    <div class="col-md-12">
      <div class="row">
        <div class="col-md-12">
          <h2><strong>Public Information Portal</strong></h2>
        </div>
      </div>
      <div class="tab-container tab-orange">
        <ul class="nav nav-tabs">
          <li class="active">
            <a data-toggle="tab", href="#all-military-equipment">All Military Equipment</a>
          </li>
          <li>
            <a data-toggle="tab", href="#posted-public-notices">Posted Public Notices</a>
          </li>
          <% if current_user %>
            <li>
              <a data-toggle="tab", href="#user-subscriptions">My Subscriptions</a>
            </li>
          <% end %>
          <li>
            <a data-toggle="tab", href="#about">About</a>
          </li>

        </ul>
        <div class="tab-content">
          <div class="tab-pane" id="user-subscriptions">
            <div class="row">
              <div class="col-md-12">
                <!-- heading -->
              </div>
            </div>
            <% if current_user %>
              <div class="row">
                <div class="col-md-12">
                  <%= render "static_pages/public/user_subscriptions" %>
                </div>
              </div>
            <% end %>
          </div>
          <div class="tab-pane" id="posted-public-notices">
            <div class="row">
              <div class="col-md-12">
                <!-- heading -->
              </div>
            </div>
            <div class="row">
              <div class="col-md-12">
                <%= render "static_pages/public/public_notices" %>
              </div>
            </div>
          </div>
          <div class="tab-pane" id="about">
            <div class="row">
              <div class="col-md-12">
<iframe width="853" height="480" src="https://www.youtube.com/embed/O9sUuU5nGtM" frameborder="0" allowfullscreen></iframe>              </div>
            </div>
          </div>
          <div class="tab-pane active" id="all-military-equipment">
            <div class="row">
              <div class="col-md-12">
                <!-- heading -->
              </div>
              <div class="row">
                <div class="col-md-12">
                  <%= render "static_pages/public/existing_equipment" %>
                </div>
              </div>
            </div>

          </div>
        </div>
      </div>
    </div>
  </div>
</div>
